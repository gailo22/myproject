<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="myproject.domain.Entry" table="entry">

		<property name="memberId" type="long">
			<column name="memberId" not-null="true"/>
		</property>
		<property name="tourId" type="Long">
			<column name="tourId" not-null="true" />
		</property>
		<property name="year" type="Long">
			<column name="year" not-null="true" />
		</property>
		
	</class>
</hibernate-mapping>